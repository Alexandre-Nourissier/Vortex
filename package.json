{
  "private": true,
  "name": "vortex_devel",
  "version": "0.0.1",
  "main": "out/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nexus-Mods/Vortex-Private"
  },
  "scripts": {
    "_assets_out": "node InstallAssets.js out",
    "_assets_app": "node InstallAssets.js app",
    "_update_submodules": "git submodule foreach git pull origin master",
    "_build_subprojects_out": "npm run _update_submodules && node BuildSubprojects.js out",
    "_build_subprojects_app": "npm run _update_submodules && node BuildSubprojects.js app",
    "_clean_app": "cd app && rm-local-modules",
    "_install_app": "cd app && npm install && npm update",
    "build_api": "cd api && npm run build && cd .. && npm install ./api",
    "build_rest": "npm run _assets_out && npm run build_api && npm run _build_subprojects_out",
    "prepackage": "npm run build_api && npm run _clean_app && npm run _install_app && npm run _build_subprojects_app && npm run _assets_app && npm install 7zip-bin",
    "webpack_main_unused": "webpack --config build/webpack.main.config.js --display-error-details --progress --profile --color",
    "webpack_renderer_unused": "webpack --config build/webpack.renderer.config.js --display-error-details --progress --profile --color",
    "package": "build",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=development electron .",
    "preinstallex": "npm install rm-local-modules && rm-local-modules",
    "installex": "npm install && npm run build",
    "preinstall": "git submodule update --init --recursive",
    "postinstall": "typings prune && typings install",
    "sassc": "node-sass src/stylesheets/index.scss out/assets/css/base.css",
    "test": "jest --no-cache",
    "doc": "typedoc --out doc src",
    "build": "tsc -p . && npm run build_rest",
    "buildwatch": "npm run build_rest && concurrently -p \"\" --kill-others \"tsc -p . -w\" npm-watch"
  },
  "watch": {},
  "engines": {
    "electron": "1.7.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|ts|tsx)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/app/"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "collectCoverage": true
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "7z.exe",
      "LICENSE.md",
      "bundledPlugins",
      "lib/modinstaller",
      "node_modules/7z-bin",
      "node_modules/bootstrap-sass/assets/stylesheets",
      "node_modules/react-select/scss",
      "assets/css/**"
    ],
    "appId": "com.nexusmods.vortex",
    "win": {
      "target": "nsis",
      "publish": [
        {
          "provider": "github",
          "token": "008e0c7bfa1a9a2f14df6370dfdac9340ec828a",
          "private": true
        }
      ]
    },
    "nsis": {
      "perMachine": true,
      "runAfterFinish": true,
      "menuCategory": true
    },
    "extraResources": [
      "locales"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin${/*}",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/*.js.map",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!**/*.{pdb,lib,VC.db,exp,gypi,d.ts}",
      "!**/build/Debug${/*}",
      "!**/build/Release/obj${/*}",
      "!**/build/.vs${/*}",
      "!**/autogypi.json",
      "!**/binding.gyp",
      "!**/binding.map",
      "!**/build/src/Release/obj${/*}",
      "!**/node_modules/edge/lib/native${/*}",
      "!**/node_modules/edge/src${/*}",
      "!**/node_modules/leveldown/deps${/*}",
      "!**/node_modules/node-sass/{build,src,vendor}${/*}",
      "!**/node_modules/react-layout-pane/{demo,docs}${/*}",
      "!**/node_modules/recharts/umd${/*}",
      "!**/node_modules/ffi/deps",
      "!**/Release/*.map",
      "!**/node_modules/bootstrap-sass/assets/fonts",
      "!**/node_modules/recharts/node_modules/d3-scale/img",
      "!**/node_modules/recharts/node_modules/d3-shape/img",
      "!**/node_modules/d3-geo/img"
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
